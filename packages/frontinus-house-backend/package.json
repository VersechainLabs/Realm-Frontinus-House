{
  "name": "frontinus-house-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && yarn sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "ts-node -r tsconfig-paths/register tasks/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "entity:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm entity:create -n",
    "migration:generate": "yarn build && yarn migration:run && yarn run typeorm migration:generate -n",
    "migration:run": "yarn build && yarn run typeorm migration:run",
    "migration:revert": "yarn build && yarn run typeorm migration:revert",
    "migration:drop": "yarn build && yarn run typeorm migration:drop",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org metaforo --project nestjs-frontinus ../frontinus-house-webapp/build && sentry-cli sourcemaps upload --org metaforo --project nestjs-frontinus ../frontinus-house-webapp/build"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^1.1.2",
    "@nestjs/core": "^10.1.3",
    "@nestjs/graphql": "^10.1.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^8.0.2",
    "@pinata/sdk": "^2.1.0",
    "@sentry/node": "^7.69.0",
    "@sentry/profiling-node": "^1.2.1",
    "apollo-server-express": "^3.10.2",
    "aws-sdk": "^2.1515.0",
    "body-parser": "^1.20.0",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cross-fetch": "^3.1.5",
    "file-type": "16.5.3",
    "graphql": "^16.6.0",
    "mime-types": "^2.1.35",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.41",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@nouns/frontinus-house-wrapper": "1.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(e2e-spec)|(spec)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
